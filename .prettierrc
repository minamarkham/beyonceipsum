{
	"trailingComma": "all",
	"tabWidth": 4,
	"semi": true,
	"singleQuote": true,
	"printWidth": 80,
	"arrowParens": "always",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"useTabs": true,
	"endOfLine": "auto",
	"overrides": [
		{
			"files": "*.json",
			"options": {
				"parser": "json",
				"singleQuote": false
			}
		}
	]
}
